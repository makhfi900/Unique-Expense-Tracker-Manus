{
  "name": "unique-expense-tracker",
  "version": "1.0.0",
  "description": "A full-stack expense tracking application with role-based access control",
  "main": "index.js",
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd frontend && npm start",
    "install-all": "npm install && cd frontend && npm install",
    "test": "cd frontend && npm test",
    "api": "node api-server.js",
    "dev:api": "nodemon api-server.js",
    "dev:full": "concurrently -n \"API,Frontend\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run dev:api\" \"npm run dev\" --restart-tries 3",
    "start:dev": "node start-development.js"
  },
  "keywords": [
    "expense-tracker",
    "react",
    "netlify",
    "supabase",
    "jwt",
    "full-stack"
  ],
  "author": "Manus AI",
  "license": "MIT",
  "devDependencies": {
    "@netlify/functions": "^2.4.1",
    "@testing-library/dom": "^10.4.1",
    "concurrently": "^9.2.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.10",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1"
  }
}
